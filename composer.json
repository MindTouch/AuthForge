{
  "name": "modethirteen/authforge",
  "description": "A collection of components to integrate industry-standard authentication and authorization",
  "type": "library",
  "license": "Apache-2.0",
  "authors" : [
    {
      "name": "James Andrew Vaughn",
      "email": "jamesvaughn@modethirteen.com",
      "role": "maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/modethirteen/AuthForge/issues"
  },
  "require": {
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-gmp": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "php": "~8.0.0",
    "modethirteen/fluent-cache": "dev-main-php8",
    "modethirteen/hyperplug": "dev-main-php8",
    "modethirteen/miniflex": "dev-main-php8",
    "modethirteen/type-ex": "dev-main-php8",
    "modethirteen/xarray": "dev-main-php8",
    "psr/http-message": "^1.0.1",
    "psr/event-dispatcher": "^1.0.0",
    "psr/log": "^1.1.3",
    "ramsey/uuid": "~4.1.1",
    "robrichards/xmlseclibs": "~3.1.1",
    "symfony/event-dispatcher-contracts": "^2.2.0",
    "web-token/jwt-checker": "~2.2.6",
    "web-token/jwt-core": "~2.2.6",
    "web-token/jwt-key-mgmt": "~2.2.6",
    "web-token/jwt-signature": "~2.2.6",
    "web-token/jwt-signature-algorithm-ecdsa": "~2.2.6",
    "web-token/jwt-signature-algorithm-hmac": "~2.2.6",
    "web-token/jwt-signature-algorithm-rsa": "~2.2.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9.3",
    "phpunit/phpunit": "~9.4.3",
    "rector/rector": "^0.15.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/fluentcache",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/miniflex",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/TypeEx",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/Xarray.php",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/HyperPlug",
      "no-api": true
    }
  ],
  "autoload": {
    "psr-4": {
      "modethirteen\\AuthForge\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "modethirteen\\AuthForge\\Tests\\": ["tests/"]
    }
  }
}
